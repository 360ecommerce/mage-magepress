<?xml version="1.0" ?>
<config>
    <modules>
        <Threesixty_Wordpress>
            <version>0.1</version>
        </Threesixty_Wordpress>
    </modules>
    <global>
        <blocks>
            <threesixty>
                <class>Threesixty_Wordpress_Block</class>
            </threesixty>
        </blocks>
        <models>
            <threesixty>
                <class>Threesixty_Wordpress_Model</class>
            </threesixty>
            <catalog>
                <rewrite>
                    <product_api>Threesixty_Wordpress_Model_Catalog_Product_Api</product_api>
                    <category_api>Threesixty_Wordpress_Model_Catalog_Category_Api</category_api>
                </rewrite>
            </catalog>
            <wordpress>
                <rewrite>
                    <menu>Threesixty_Wordpress_Model_Menu</menu>
                    <menu_item>Threesixty_Wordpress_Model_Menu_Item</menu_item>
                </rewrite>
            </wordpress>
        </models>
        <helpers>
            <threesixty>
                <class>Threesixty_Wordpress_Helper</class>
            </threesixty>
        </helpers>
    </global>
</config>